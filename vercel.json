{
  "functions": {
    "api/loads.go": {
      "runtime": "go1.x"
    }
  },
  "routes": [
    {
      "src": "/api/loads",
      "dest": "/api/loads.go"
    },
    {
      "src": "/frontend/(.*)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
